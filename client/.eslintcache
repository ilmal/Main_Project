=>{\n            this.setState({loadAnimation: true})\n        }, 50)\n\n        const changeGame = ()=>{\n            const list = [\"Minecraft\", \"ARK\", \"Terraria\", \"Unturned\", \"Rust\"]\n            this.setState({games:list[this.state.i]})\n            this.setState({i:this.state.i + 1})\n            console.log(list[this.state.i])\n            if(this.state.i === list.length){\n                this.setState({i:0})\n            }\n            setTimeout(changeGame, 3000)\n        }\n        changeGame()\n    }\n\n    page1 = ()=>{\n        return(\n            <div className=\"homeMainContainer\">\n                <div className=\"innerContainer\">\n                    <span className={this.state.loadAnimation? \"homeMainSpan\":\"homeMainSpanAway\"}>Get your <span className=\"homeChangeGameSpan\">{this.state.games}</span> server from U1Servers</span>\n                </div>\n                <div className=\"homePageArrow fas fa-angle-double-down\" onClick={this.pageDown}/>\n            </div>\n        )\n    }\n\n    page2 = ()=>{\n        return(\n            <div className=\"homePage2MainContaiener\">\n                <div className=\"homePage2Title\">\n                    <span>Choose your game</span>\n                </div>\n                <div className=\"homePage2Card\">\n                    <div className=\"homePage2Card1\">\n                        <span>Ark</span>\n                    </div>\n                    <div className=\"homePage2Card2\">\n                        <span>Minecraft</span>\n                    </div>\n                    <div className=\"homePage2Card3\">\n                        <span>Terraria</span>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n    errFunc = ()=>{\n        return(\n            <div className=\"homeMainContainer\">\n                <div className=\"innerContainer\">\n                    <span className=\"homePageError\">error occured</span>\n                </di